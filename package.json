{
	"name": "panther-press",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"dhost": "astro dev --host",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky"
	},
	"dependencies": {
		"@astrojs/cloudflare": "^11.1.0",
		"@astrojs/react": "^3.6.2",
		"@astrojs/rss": "4.0.7",
		"@fontsource-variable/inter": "^5.1.0",
		"@fontsource-variable/manrope": "^5.1.0",
		"@fontsource/atkinson-hyperlegible": "^5.1.0",
		"@keystatic/astro": "^5.0.0",
		"@keystatic/core": "^0.5.36",
		"@preline/collapse": "^2.5.0",
		"@preline/overlay": "^2.5.0",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.0",
		"astro": "4.15.11",
		"framer-motion": "^11.11.8",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@astrojs/mdx": "3.1.7",
		"@astrojs/sitemap": "3.2.0",
		"@astrojs/tailwind": "5.1.1",
		"@pagefind/default-ui": "^1.1.1",
		"@tailwindcss/typography": "0.5.15",
		"@typescript-eslint/parser": "^8.8.0",
		"clsx": "2.1.1",
		"eslint": "^9.11.1",
		"eslint-plugin-astro": "^1.2.4",
		"eslint-plugin-jsx-a11y": "^6.10.0",
		"husky": "^9.1.6",
		"lint-staged": "^15.2.10",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^10.18.0",
		"pagefind": "^1.1.1",
		"prettier": "^3.3.3",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.14.1",
		"slugify": "^1.6.6",
		"tailwind-merge": "2.5.3",
		"tailwindcss": "3.4.13",
		"typescript": "^5.6.2"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=. --ignore-path=.prettierignore --no-error-on-unmatched-pattern"
		]
	}
}
