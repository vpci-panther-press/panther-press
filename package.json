{
	"name": "panther-press",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "tinacms dev -c \"astro dev\"",
		"dhost": "tinacms dev -c \"astro dev --host\"",
		"start": "astro dev",
		"build": "tinacms build && astro build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@astrojs/rss": "4.0.5",
		"@fontsource-variable/inter": "^5.0.16",
		"@fontsource-variable/manrope": "^5.0.18",
		"@fontsource/atkinson-hyperlegible": "^5.0.18",
		"@preline/collapse": "^2.0.2",
		"@preline/overlay": "^2.0.2",
		"astro": "4.4.0",
		"tinacms": "^1.5.28"
	},
	"devDependencies": {
		"@astrojs/mdx": "2.1.1",
		"@astrojs/sitemap": "3.1.4",
		"@astrojs/tailwind": "5.1.0",
		"@pagefind/default-ui": "^1.0.4",
		"@tailwindcss/typography": "0.5.10",
		"@tinacms/cli": "^1.5.39",
		"@typescript-eslint/parser": "^7.0.1",
		"clsx": "2.1.0",
		"eslint": "^8.56.0",
		"eslint-plugin-astro": "^0.31.4",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^10.17.0",
		"pagefind": "^1.0.4",
		"prettier": "^3.2.5",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.13.0",
		"reading-time": "^1.5.0",
		"slugify": "^1.6.6",
		"tailwind-merge": "2.2.1",
		"tailwindcss": "3.4.1",
		"typescript": "^5.3.3"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=. --ignore-path=.prettierignore --no-error-on-unmatched-pattern"
		]
	}
}
