{
	"name": "panther-press",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"dhost": "astro dev --host",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "astro preview",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky install"
	},
	"dependencies": {
		"@astrojs/cloudflare": "^11.0.1",
		"@astrojs/react": "^3.6.0",
		"@astrojs/rss": "4.0.7",
		"@fontsource-variable/inter": "^5.0.18",
		"@fontsource-variable/manrope": "^5.0.20",
		"@fontsource/atkinson-hyperlegible": "^5.0.20",
		"@keystatic/astro": "^5.0.0",
		"@keystatic/core": "^0.5.23",
		"@preline/collapse": "^2.3.0",
		"@preline/overlay": "^2.3.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"astro": "4.14.5",
		"framer-motion": "^11.2.12",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@astrojs/mdx": "3.1.2",
		"@astrojs/sitemap": "3.1.6",
		"@astrojs/tailwind": "5.1.0",
		"@pagefind/default-ui": "^1.1.0",
		"@tailwindcss/typography": "0.5.13",
		"@typescript-eslint/parser": "^7.15.0",
		"clsx": "2.1.1",
		"eslint": "^9.6.0",
		"eslint-plugin-astro": "^1.2.2",
		"eslint-plugin-jsx-a11y": "^6.9.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.7",
		"mdast-util-to-string": "^4.0.0",
		"motion": "^10.18.0",
		"pagefind": "^1.1.0",
		"prettier": "^3.3.2",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.14.0",
		"slugify": "^1.6.6",
		"tailwind-merge": "2.3.0",
		"tailwindcss": "3.4.4",
		"typescript": "^5.5.3"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=. --ignore-path=.prettierignore --no-error-on-unmatched-pattern"
		]
	}
}
