{
	"name": "panther-press",
	"version": "1.1.0",
	"private": true,
	"scripts": {
		"dev": "astro dev",
		"dhost": "astro dev --host",
		"start": "astro dev",
		"build": "astro build",
		"sync": "astro sync",
		"preview": "wrangler pages dev ./dist",
		"postbuild": "pagefind --site dist",
		"format:check": "prettier --plugin-search-dir=. --check .",
		"format": "prettier --plugin-search-dir=. --write .",
		"lint": "eslint .",
		"pre-commit": "lint-staged",
		"prepare": "husky"
	},
	"dependencies": {
		"@astrojs/cloudflare": "^11.2.0",
		"@astrojs/react": "^3.6.3",
		"@astrojs/rss": "4.0.9",
		"@fontsource-variable/inter": "^5.1.0",
		"@fontsource-variable/manrope": "^5.1.0",
		"@fontsource/atkinson-hyperlegible": "^5.1.0",
		"@keystatic/astro": "^5.0.3",
		"@keystatic/core": "^0.5.41",
		"@preline/collapse": "^2.5.0",
		"astro": "4.16.14",
		"framer-motion": "^11.12.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1"
	},
	"devDependencies": {
		"@astrojs/mdx": "3.1.9",
		"@astrojs/sitemap": "3.2.1",
		"@astrojs/tailwind": "5.1.2",
		"@pagefind/default-ui": "^1.2.0",
		"@tailwindcss/typography": "0.5.15",
		"@types/eslint-plugin-jsx-a11y": "~6.10.0",
		"@types/lint-staged": "~13.3.0",
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"@typescript-eslint/parser": "^8.16.0",
		"clsx": "2.1.1",
		"eslint": "^9.15.0",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"husky": "^9.1.7",
		"lint-staged": "^15.2.10",
		"mdast-util-to-string": "^4.0.0",
		"pagefind": "^1.2.0",
		"prettier": "^3.3.3",
		"prettier-config-standard": "^7.0.0",
		"prettier-plugin-astro": "^0.14.1",
		"tailwind-merge": "2.5.5",
		"tailwindcss": "3.4.15",
		"typescript": "^5.7.2",
		"wrangler": "^3.90.0"
	},
	"lint-staged": {
		"*.{astro,js,jsx,ts,tsx,md,mdx,json}": [
			"prettier --write --plugin-search-dir=. --ignore-path=.prettierignore --no-error-on-unmatched-pattern"
		]
	}
}
